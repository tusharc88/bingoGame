{
  "name": "bingo-game",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "Bingo Game Application",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config=internals/webpack/webpack.config.dev.js --open --hot",
    "build": "cross-env NODE_ENV=production webpack --config=internals/webpack/webpack.config.prod.js --optimize-minimize",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json",
    "test:watch": "cross-env NODE_ENV=text jest --config=jest.config.json --watchAll",
    "lint": "eslint src internals",
    "lint:css": "stylelint \"src/**/*.js\"",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006 -c internals/storybook",
    "precommit": "npm run lint && npm run lint:css && lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write \"src/**/*.js\"",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
